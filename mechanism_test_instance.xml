 xmlns:DNSRecordObj="http://cybox.mitre.org/objects#DNSRecordObject-2"
 xmlns:antiBehavioralAnalysisMechanism="http://maec.mitre.org/XMLSchema/maec-mechanisms#antibehavioralanalysis-1"
 xmlns:c2Mechanism="http://maec.mitre.org/XMLSchema/maec-mechanisms#c2-1"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://maec.mitre.org/XMLSchema/maec-bundle-4 file:maec_bundle_schema.xsd
 http://maec.mitre.org/XMLSchema/maec-mechanisms#c2-1 file:mechanisms/c2_mechanism_schema.xsd
 http://maec.mitre.org/XMLSchema/maec-mechanisms#antibehavioralanalysis-1 file:mechanisms/anti_behavioral_analysis_mechanism_schema.xsd" 
 id="maec-test-bnd-1" schema_version="4.0.1" defined_subject="false">
 <maecBundle:Mechanisms>
  <maecBundle:Mechanism id="maec-tst-mch-1" name="spying">
    <maecBundle:Strategic_Objective id="maec-tst-obj-1">
     <maecBundle:Name xsi:type="maecVocabs:SpyingStrategicObjectiveVocab-1.0">capture user input</maecBundle:Name>
    </maecBundle:Strategic_Objective>
   <maecBundle:Tactical_Objective id="maec-tst-obj-2">
    <maecBundle:Name xsi:type="maecVocabs:SpyingTacticalObjectiveVocab-1.0">capture mouse input</maecBundle:Name>
    <maecBundle:Relationship>
     <maecBundle:Relationship_Type>child of</maecBundle:Relationship_Type>
     <maecBundle:Objective_Reference objective_idref="maec-tst-obj-1"/>
    </maecBundle:Relationship>
   </maecBundle:Tactical_Objective>
   <maecBundle:Tactical_Objective id="maec-tst-obj-3">
    <maecBundle:Name xsi:type="maecVocabs:SpyingTacticalObjectiveVocab-1.0">capture keyboard input</maecBundle:Name>
    <maecBundle:Relationship>
     <maecBundle:Relationship_Type>child of</maecBundle:Relationship_Type>
     <maecBundle:Objective_Reference objective_idref="maec-tst-obj-1"/>
    </maecBundle:Relationship>
   </maecBundle:Tactical_Objective>
  </maecBundle:Mechanism>
  
  <maecBundle:Mechanism id="maec-test-mch-2" name="botnet command and control">
   <maecBundle:Properties xsi:type="c2Mechanism:BotC2MechanismPropertiesType" is_proxy_aware="true" uses_fluxing="false">
    <c2Mechanism:Utilized_Protocol>HTTP</c2Mechanism:Utilized_Protocol>
   </maecBundle:Properties>
    <maecBundle:Strategic_Objective id="maec-tst-obj-4">
     <maecBundle:Name xsi:type="maecVocabs:BotnetCommandandControlStrategicObjectiveVocab-1.0">communicate with servers</maecBundle:Name>
    </maecBundle:Strategic_Objective>
  </maecBundle:Mechanism>
  
  <maecBundle:Mechanism id="maec-test-mch-3" name="anti behavioral analysis">
   <maecBundle:Strategic_Objective id="maec-test-obj-5">
    <maecBundle:Name xsi:type="maecVocabs:AntiBehavioralAnalysisStrategicObjectiveVocab-1.0">anti-sandbox</maecBundle:Name>
   </maecBundle:Strategic_Objective>
   <maecBundle:Tactical_Objective id="maec-tst-obj-6">
    <maecBundle:Name xsi:type="maecVocabs:AntiBehavioralAnalysisTacticalObjectiveVocab-1.0">sandbox environment detection</maecBundle:Name>
    <maecBundle:Properties xsi:type="antiBehavioralAnalysisMechanism:AntiSandboxTacticalObjectivePropertiesType">
     <antiBehavioralAnalysisMechanism:Affected_Sandbox>
      <cyboxCommon:Name>some sandbox</cyboxCommon:Name>
      <cyboxCommon:Version>1.2.3</cyboxCommon:Version>
     </antiBehavioralAnalysisMechanism:Affected_Sandbox>
    </maecBundle:Properties>
   </maecBundle:Tactical_Objective>
  </maecBundle:Mechanism>
 </maecBundle:Mechanisms>
</maecBundle:MAEC_Bundle>
