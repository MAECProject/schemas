<?xml version="1.0" encoding="UTF-8"?>
<!--
 MAEC Container Multiple Packages Example
 -Demonstrates use of the Container for capturing multiple Packages
 -The captured Malware Subjects here are related to two families of malware
 
 Created November 2012 for MAEC v3.0 and CybOX v1.0 final-->
<maecContainer:MAEC_Container
 xmlns:maecContainer="http://maec.mitre.org/XMLSchema/maec-container-1"
 xmlns:maecPackage="http://maec.mitre.org/XMLSchema/maec-package-1"
 xmlns:metadata="http://xml/metadataSharing.xsd"
 xmlns:cybox="http://cybox.mitre.org/cybox_v1"
 xmlns:Common="http://cybox.mitre.org/Common_v1"
 xmlns:FileObj="http://cybox.mitre.org/objects#FileObject"
 xmlns:WinExecutableFileObj="http://cybox.mitre.org/objects#WinExecutableFileObject"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://maec.mitre.org/XMLSchema/maec-unit-1 ../maec-container-schema.xsd
 http://cybox.mitre.org/objects#WinExecutableFileObject http://cybox.mitre.org/XMLSchema/objects/Win_Executable_File/Win_Executable_File_Object_1.3.xsd"
 id="maec-example-con-1" schema_version="1.0">
    <maecContainer:Packages>
        <maecContainer:Package id="maec-example-pkg-1" schema_version="1.0">
            <maecPackage:Malware_Subjects>
                <maecPackage:Malware_Subject id="maec-example-sub-1">
                    <maecPackage:Malware_Instance_Object_Attributes>
                        <cybox:Defined_Object xsi:type="WinExecutableFileObj:WindowsExecutableFileObjectType">
                            <FileObj:Hashes>
                                <Common:Hash>
                                    <Common:Type datatype="String">MD5</Common:Type>
                                    <Common:Simple_Hash_Value>32530b68d78e5bcbd73a138276f45490</Common:Simple_Hash_Value>
                                </Common:Hash>
                            </FileObj:Hashes>
                        </cybox:Defined_Object>
                    </maecPackage:Malware_Instance_Object_Attributes>
                </maecPackage:Malware_Subject>
                
                <maecPackage:Malware_Subject id="maec-example-sub-2">
                    <maecPackage:Malware_Instance_Object_Attributes>
                        <cybox:Defined_Object xsi:type="WinExecutableFileObj:WindowsExecutableFileObjectType">
                            <FileObj:Hashes>
                                <Common:Hash>
                                    <Common:Type datatype="String">MD5</Common:Type>
                                    <Common:Simple_Hash_Value>05a2d7b4d816c392632e40ac34a9d886</Common:Simple_Hash_Value>
                                </Common:Hash>
                            </FileObj:Hashes>
                        </cybox:Defined_Object>
                    </maecPackage:Malware_Instance_Object_Attributes>
                </maecPackage:Malware_Subject>
            </maecPackage:Malware_Subjects>
            
            <maecPackage:Grouping_Relationships>
                <maecPackage:Grouping_Relationship type="same_malware_family">
                    <maecPackage:Malware_Family_Name>Eyestye</maecPackage:Malware_Family_Name>
                </maecPackage:Grouping_Relationship>
            </maecPackage:Grouping_Relationships>
        </maecContainer:Package>
        
        <maecContainer:Package id="maec-example-pkg-2" schema_version="1.0">
            <maecPackage:Malware_Subjects>
                <maecPackage:Malware_Subject id="maec-example-sub-3">
                    <maecPackage:Malware_Instance_Object_Attributes>
                        <cybox:Defined_Object xsi:type="WinExecutableFileObj:WindowsExecutableFileObjectType">
                            <FileObj:Hashes>
                                <Common:Hash>
                                    <Common:Type datatype="String">MD5</Common:Type>
                                    <Common:Simple_Hash_Value>05a2d7b4d816c392632e40ac34a9d886</Common:Simple_Hash_Value>
                                </Common:Hash>
                            </FileObj:Hashes>
                        </cybox:Defined_Object>
                    </maecPackage:Malware_Instance_Object_Attributes>
                </maecPackage:Malware_Subject>
                
                <maecPackage:Malware_Subject id="maec-example-sub-4">
                    <maecPackage:Malware_Instance_Object_Attributes>
                        <cybox:Defined_Object xsi:type="WinExecutableFileObj:WindowsExecutableFileObjectType">
                            <FileObj:Hashes>
                                <Common:Hash>
                                    <Common:Type datatype="String">MD5</Common:Type>
                                    <Common:Simple_Hash_Value>35a02499c0dfc1353717dd224d3ac87f</Common:Simple_Hash_Value>
                                </Common:Hash>
                            </FileObj:Hashes>
                        </cybox:Defined_Object>
                    </maecPackage:Malware_Instance_Object_Attributes>
                </maecPackage:Malware_Subject>
            </maecPackage:Malware_Subjects>
            
            <maecPackage:Grouping_Relationships>
                <maecPackage:Grouping_Relationship type="same_malware_family">
                    <maecPackage:Malware_Family_Name>Zeus</maecPackage:Malware_Family_Name>
                </maecPackage:Grouping_Relationship>
            </maecPackage:Grouping_Relationships>
        </maecContainer:Package>
    </maecContainer:Packages>
</maecContainer:MAEC_Container>
