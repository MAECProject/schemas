 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 id="maec-example-bnd-5" schema_version="3.0" xsi:schemaLocation="http://maec.mitre.org/XMLSchema/maec-bundle-3 file:../maec-bundle-schema.xsd
 http://cybox.mitre.org/objects#WinExecutableFileObject http://cybox.mitre.org/XMLSchema/objects/Win_Executable_File/Win_Executable_File_Object_1.2.xsd
 http://cybox.mitre.org/objects#WinRegistryKeyObject http://cybox.mitre.org/XMLSchema/objects/Win_Registry_Key/Win_Registry_Key_Object_1.2.xsd
 http://cybox.mitre.org/objects#WinMutexObject http://cybox.mitre.org/XMLSchema/objects/Win_Mutex/Win_Mutex_Object_1.1.xsd
 http://cybox.mitre.org/objects#WinProcessObject http://cybox.mitre.org/XMLSchema/objects/Win_Process/Win_Process_Object_1.2.xsd" defined_subject="true" content_type="dynamic_analysis_output">
<maecBundle:Subject_Attributes>
 <cybox:Defined_Object xsi:type="WinExecutableFileObj:WindowsExecutableFileObjectType">
  <FileObj:Size_In_Bytes datatype="UnsignedLong">332288</FileObj:Size_In_Bytes>
  <FileObj:Hashes>
   <Common:Hash>
    <Common:Type datatype="String">MD5</Common:Type>
    <Common:Simple_Hash_Value datatype="hexBinary">6519ED23FB22D0228E61A68D40F75C49</Common:Simple_Hash_Value>
   </Common:Hash>
   <Common:Hash>
    <Common:Type datatype="String">SHA1</Common:Type>
    <Common:Simple_Hash_Value datatype="hexBinary">7F42DF6B1318A5CC4633E33076629F95F59F1445</Common:Simple_Hash_Value>
   </Common:Hash>
  </FileObj:Hashes>
 </cybox:Defined_Object>
</maecBundle:Subject_Attributes>
 
<maecBundle:Actions>
 <maecBundle:Action type="Create" id="maec-example-act-1">
  <cybox:Action_Name>
   <cybox:Defined_Name>Create File</cybox:Defined_Name>
  </cybox:Action_Name>
  <cybox:Associated_Objects>
   <cybox:Associated_Object association_type="Affected" id="maec-example-obj-1">
    <cybox:Defined_Object xsi:type="WinExecutableFileObj:WindowsExecutableFileObjectType">
     <FileObj:File_Name datatype="String">Zcxaxz.exe</FileObj:File_Name>
     <FileObj:File_Path fully_qualified="false" datatype="String">%AppData%\</FileObj:File_Path>
     <FileObj:Size_In_Bytes datatype="UnsignedLong">332288</FileObj:Size_In_Bytes>
    </cybox:Defined_Object>
   </cybox:Associated_Object>
  </cybox:Associated_Objects>
 </maecBundle:Action>
 
 <maecBundle:Action type="Create" id="maec-example-act-2">
  <cybox:Action_Name>
   <cybox:Defined_Name>Create Registry Key</cybox:Defined_Name>
  </cybox:Action_Name>
  <cybox:Associated_Objects>
   <cybox:Associated_Object association_type="Affected" id="maec-example-obj-2">
    <cybox:Defined_Object xsi:type="WinRegistryKeyObj:WindowsRegistryKeyObjectType">
     <WinRegistryKeyObj:Key datatype="String">Software\Microsoft\Windows\CurrentVersion\Run</WinRegistryKeyObj:Key>
     <WinRegistryKeyObj:Hive datatype="String">HKEY_CURRENT_USER</WinRegistryKeyObj:Hive>
     <WinRegistryKeyObj:Values>
      <WinRegistryKeyObj:Value>
       <WinRegistryKeyObj:Name datatype="String">Zcxaxz</WinRegistryKeyObj:Name>
       <WinRegistryKeyObj:Data datatype="String">"%AppData%\Zcxaxz.exe"</WinRegistryKeyObj:Data>
      </WinRegistryKeyObj:Value>
     </WinRegistryKeyObj:Values>
    </cybox:Defined_Object>
   </cybox:Associated_Object>
  </cybox:Associated_Objects>
 </maecBundle:Action>
 
 <maecBundle:Action type="Create" id="maec-example-act-3">
  <cybox:Action_Name>
   <cybox:Defined_Name>Create Mutex</cybox:Defined_Name>
  </cybox:Action_Name>
  <cybox:Associated_Objects>
   <cybox:Associated_Object association_type="Affected" id="maec-example-obj-3">
    <cybox:Defined_Object xsi:type="WinMutexObj:WindowsMutexObjectType" named="true">
     <MutexObj:Name datatype="String">redem-Mutex</MutexObj:Name>
    </cybox:Defined_Object>
   </cybox:Associated_Object>
  </cybox:Associated_Objects>
 </maecBundle:Action>
 
 <maecBundle:Action type="Create" id="maec-example-act-4">
  <cybox:Action_Name>
   <cybox:Undefined_Name>Create Memory Page in Process</cybox:Undefined_Name>
  </cybox:Action_Name>
  <cybox:Associated_Objects>
   <cybox:Associated_Object association_type="Affected" id="maec-example-obj-4">
    <cybox:Defined_Object xsi:type="WinProcessObj:WindowsProcessObjectType">
     <ProcessObj:Name datatype="String">services.exe</ProcessObj:Name>
     <ProcessObj:Image_Info>
      <ProcessObj:Path datatype="String">%System%\services.exe</ProcessObj:Path>
     </ProcessObj:Image_Info>
    </cybox:Defined_Object>
   </cybox:Associated_Object>
  </cybox:Associated_Objects>
 </maecBundle:Action>
 
 <maecBundle:Action type="Create" id="maec-example-act-5">
  <cybox:Action_Name>
   <cybox:Undefined_Name>Create Memory Page in Process</cybox:Undefined_Name>
  </cybox:Action_Name>
  <cybox:Associated_Objects>
   <cybox:Associated_Object association_type="Affected" id="maec-example-obj-5">
    <cybox:Defined_Object xsi:type="WinProcessObj:WindowsProcessObjectType">
     <ProcessObj:Name datatype="String">svchost.exe</ProcessObj:Name>
     <ProcessObj:Image_Info>
      <ProcessObj:Path datatype="String">%System%\svchost.exe</ProcessObj:Path>
     </ProcessObj:Image_Info>
    </cybox:Defined_Object>
   </cybox:Associated_Object>
  </cybox:Associated_Objects>
 </maecBundle:Action>
 
 <maecBundle:Action type="Create" id="maec-example-act-6">
  <cybox:Action_Name>
   <cybox:Undefined_Name>Create Memory Page in Process</cybox:Undefined_Name>
  </cybox:Action_Name>
  <cybox:Associated_Objects>
   <cybox:Associated_Object association_type="Affected" id="maec-example-obj-6">
    <cybox:Defined_Object xsi:type="WinProcessObj:WindowsProcessObjectType">
     <ProcessObj:Name datatype="String">alg.exe</ProcessObj:Name>
     <ProcessObj:Image_Info>
      <ProcessObj:Path datatype="String">%System%\alg.exe</ProcessObj:Path>
     </ProcessObj:Image_Info>
    </cybox:Defined_Object>
   </cybox:Associated_Object>
  </cybox:Associated_Objects>
 </maecBundle:Action>
</maecBundle:Actions>
 
 <maecBundle:Candidate_Indicators>
  <!-- An example of a simple object-based candidate indicator for the characterized malware instance -->
  <maecBundle:Candidate_Indicator id="maec-example-ind-1">
   <maecBundle:Composition operator="OR">
    <maecBundle:Sub_Composition operator="AND">
     <maecBundle:Object_Reference object_idref="maec-example-obj-1"/>
     <maecBundle:Object_Reference object_idref="maec-example-obj-2"/>
    </maecBundle:Sub_Composition>
    <maecBundle:Sub_Composition operator="OR">
     <maecBundle:Object_Reference object_idref="maec-example-obj-3"/>
    </maecBundle:Sub_Composition>
   </maecBundle:Composition>
  </maecBundle:Candidate_Indicator>
 </maecBundle:Candidate_Indicators>
</maecBundle:MAEC_Bundle>
