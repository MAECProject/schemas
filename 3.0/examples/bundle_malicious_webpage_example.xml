<?xml version="1.0" encoding="UTF-8"?>
<!--
 MAEC Bundle Malicious Webpage Analysis Example
 -Demonstrates how a MAEC Bundle can be used to capture the malicious aspects of a webpage
 -Based on a Wepawet report: http://wepawet.cs.ucsb.edu/view.php?hash=aa6b3e11dbef24845608e5c7dae806d2&t=1333440059&type=js
 -Unlike most of the other examples, the subject here is a URL and not a file
 
 Created November 2012 for MAEC v3.0 and CybOX v1.0 final-->
<maecBundle:MAEC_Bundle 
 xmlns:Common="http://cybox.mitre.org/Common_v1"
 xmlns:maecBundle="http://maec.mitre.org/XMLSchema/maec-bundle-3"
 xmlns:cybox="http://cybox.mitre.org/cybox_v1"
 xmlns:URIObj="http://cybox.mitre.org/objects#URIObject"
 xmlns:FileObj="http://cybox.mitre.org/objects#FileObject"
 xmlns:CodeObj="http://cybox.mitre.org/objects#CodeObject"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 id="maec-example-bnd-1" schema_version="3.0" xsi:schemaLocation="http://maec.mitre.org/XMLSchema/maec-bundle-3 file:../maec-bundle-schema.xsd
 http://cybox.mitre.org/objects#URIObject http://cybox.mitre.org/XMLSchema/objects/URI/URI_Object_1.2.xsd
 http://cybox.mitre.org/objects#ArtifactObject http://cybox.mitre.org/XMLSchema/objects/Artifact/Artifact_Object_1.0.xsd
 http://cybox.mitre.org/objects#CodeObject http://cybox.mitre.org/XMLSchema/objects/Code/Code_Object_1.1.xsd
 http://cybox.mitre.org/objects#FileObject http://cybox.mitre.org/XMLSchema/objects/File/File_Object_1.3.xsd" defined_subject="true" content_type="static_analysis_tool_output">
<maecBundle:Subject_Attributes>
 <cybox:Defined_Object xsi:type="URIObj:URIObjectType">
  <URIObj:Value>http://samsonikonyou.ru:8080/navigator/jueoaritjuir.php</URIObj:Value>
 </cybox:Defined_Object>
</maecBundle:Subject_Attributes>
 
<maecBundle:Behaviors>
 <maecBundle:Behavior id="maec-example-bhv-1">
  <maecBundle:Purpose>
   <maecBundle:Vulnerability_Exploit>
    <maecBundle:CVE cve_id="CVE-2010-0188"/>
   </maecBundle:Vulnerability_Exploit>
  </maecBundle:Purpose>
  <maecBundle:Description>A vulnerability exploit behavior initiated by some code on the web page.</maecBundle:Description>
 </maecBundle:Behavior>
 
 <maecBundle:Behavior id="maec-example-bhv-2">
  <maecBundle:Purpose>
   <maecBundle:Vulnerability_Exploit>
    <maecBundle:CVE cve_id="CVE-2012-0507"/>
   </maecBundle:Vulnerability_Exploit>
  </maecBundle:Purpose>
  <maecBundle:Description>A vulnerability exploit behavior initiated by some code on the web page.</maecBundle:Description>
 </maecBundle:Behavior>
</maecBundle:Behaviors>
 
 <maecBundle:Actions>
  <maecBundle:Action id="maec-example-act-1" type="Download" name="Download File">
    <cybox:Associated_Objects>
     <cybox:Associated_Object id="maec-example-obj-1">
      <cybox:Defined_Object xsi:type="FileObj:FileObjectType">
       <FileObj:File_Name>xodmeqglxldnx.jar</FileObj:File_Name>
      </cybox:Defined_Object>
      <cybox:Related_Objects>
       <cybox:Related_Object  id="maec-example-obj-2" relationship="Downloaded_From">
        <cybox:Defined_Object xsi:type="URIObj:URIObjectType">
         <URIObj:Value>http://samsonikonyou.ru:8080/navigator/xodmeqglxldnx.jar</URIObj:Value>
        </cybox:Defined_Object>
       </cybox:Related_Object>
      </cybox:Related_Objects>
     </cybox:Associated_Object>
    </cybox:Associated_Objects>
  </maecBundle:Action>
  
  <maecBundle:Action id="maec-example-act-2" type="Download" name="Download File">
   <cybox:Associated_Objects>
    <cybox:Associated_Object id="maec-example-obj-3">
     <cybox:Defined_Object xsi:type="FileObj:FileObjectType">
      <FileObj:File_Name>krgobpevcpync.jar</FileObj:File_Name>
     </cybox:Defined_Object>
     <cybox:Related_Objects>
      <cybox:Related_Object id="maec-example-obj-4" relationship="Downloaded_From">
       <cybox:Defined_Object xsi:type="URIObj:URIObjectType">
        <URIObj:Value>http://samsonikonyou.ru:8080/navigator/krgobpevcpync.jar</URIObj:Value>
       </cybox:Defined_Object>
      </cybox:Related_Object>
     </cybox:Related_Objects>
    </cybox:Associated_Object>
   </cybox:Associated_Objects>
  </maecBundle:Action>
  
  <maecBundle:Action id="maec-example-act-3" type="Download" name="Download File">
   <cybox:Associated_Objects>
    <cybox:Associated_Object id="maec-example-obj-5">
     <cybox:Defined_Object xsi:type="FileObj:FileObjectType">
      <FileObj:File_Name>aqcphscwgqhvkzy5.pdf</FileObj:File_Name>
     </cybox:Defined_Object>
     <cybox:Related_Objects>
      <cybox:Related_Object id="maec-example-obj-6" relationship="Downloaded_From">
       <cybox:Defined_Object xsi:type="URIObj:URIObjectType">
        <URIObj:Value>http://samsonikonyou.ru:8080/navigator/aqcphscwgqhvkzy5.pdf</URIObj:Value>
       </cybox:Defined_Object>
      </cybox:Related_Object>
      <cybox:Related_Object id="maec-example-obj-7" relationship="Contains">
       <cybox:Defined_Object xsi:type="CodeObj:CodeObjectType">
        <CodeObj:Purpose>Shellcode</CodeObj:Purpose>
        <CodeObj:Code_Language>Assembly</CodeObj:Code_Language>
        <CodeObj:XOR_Pattern>55AA55AA55AA55BB</CodeObj:XOR_Pattern>
        <CodeObj:Code_Segment_XOR><![CDATA[]]></CodeObj:Code_Segment_XOR>        
       </cybox:Defined_Object>
      </cybox:Related_Object>
     </cybox:Related_Objects>
    </cybox:Associated_Object>
   </cybox:Associated_Objects>
  </maecBundle:Action>
 </maecBundle:Actions>
</maecBundle:MAEC_Bundle>
